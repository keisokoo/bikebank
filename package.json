{
  "name": "bikebank-kr",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "parcel pages/index.pug",
    "build": "rm -rf site && parcel build pages/index.pug --dist-dir site --no-source-maps && prettier -w site/**/*.html site/css/**/*.css && cp public/* site"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "swiper": "^8.2.2"
  },
  "devDependencies": {
    "@parcel/transformer-pug": "2.6.0",
    "@parcel/transformer-sass": "2.6.0",
    "parcel": "^2.6.0",
    "parcel-namer-struct": "^1.0.0",
    "prettier": "^2.6.2",
    "pug": "^3.0.2",
    "sass": "^1.52.2",
    "typescript": "^4.7.3"
  },
  "parcel-namer-struct": [
    {
      "type": [
        "png",
        "jpg",
        "jpeg",
        "svg"
      ],
      "dest": "/images/"
    },
    {
      "type": [
        "js",
        "js.map"
      ],
      "dest": "/js/"
    },
    {
      "type": [
        "css",
        "css.map"
      ],
      "dest": "/css/"
    }
  ]
}
